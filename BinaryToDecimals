let num = "11";

function setup() {
  createCanvas(400, 100);
  binarytodecimal(num)
}

function draw() {
  background(220);
}

function binarytodecimal(val) {
  let sum = 0;
  for (let i = 0; i < val.length; i++) {
  let bit = val.charAt(val.length-i-1);
    sum += parseInt(bit) * pow(2, i);
  }
  console.log(sum);
}
